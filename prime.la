(set "number" (int (input "input-a-number")));
(set "isprime" "none");

(if (= $number 0) ({
    (set "isprime" 0)
));
(if (= $number 1) ({
    (set "isprime" 0)
));

(set "i" 2);
(while (< $i $number) ({
    (if (= (% $number $i) 0) ({
        (set "isprime" 0)
    ))
    (set "i" (+ $i 1))
));

(if (= $isprime "none") ({
    (set "isprime" 1)
));

(if (= $isprime 1) ({ (print "number-is-prime")));
(if (= $isprime 0) ({ (print "number-is-not-prime")));