(set "number" (int (input "input-a-number")));
(set "isprime" "none");

(if (= $number 0) (exelist
    (set "isprime" 0)
));
(if (= $number 1) (exelist
    (set "isprime" 0)
));

(set "i" 2);
(while (< $i $number) (exelist
    (print $i)
    (if (= (% $number $i) 0) (exelist
        (set "isprime" 0)
    ))
    (set "i" (+ $i 1))
));

(if (= $isprime "none") (exelist
    (set "isprime" 1)
));

(if (= $isprime 1) (exelist (print "number-is-prime")));
(if (= $isprime 0) (exelist (print "number-is-not-prime")));
